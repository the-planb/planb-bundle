{
  "name": "planb/planb-bundle",
  "description": "planb bundle",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "PlanB\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PlanB\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "jmpantoja",
      "email": "jmpantoja@gmail.com"
    }
  ],
  "config": {
    "bin-dir": "bin"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/the-planb/planb"
    }
  ],
  "require": {
    "php": ">=8.2",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "planb/planb": "^1.0",
    "api-platform/core": "^4.0",
    "symfony/uid": "^7.2",
    "symfony/http-kernel": "^7.2",
    "symfony/config": "^7.2",
    "symfony/yaml": "^7.2",
    "symfony/finder": "^7.2",
    "symfony/dependency-injection": "^7.2",
    "jetbrains/phpstorm-attributes": "^1.0",
    "doctrine/dbal": "^3.3",
    "nesbot/carbon": "^3.8",
    "symfony/serializer": "^7.2",
    "league/tactician": "^1.1",
    "doctrine/doctrine-bundle": "^2.7",
    "doctrine/orm": "^2.13",
    "doctrine/doctrine-fixtures-bundle": "^3.4",
    "fakerphp/faker": "^1.20",
    "symfony/security-core": "^7.2",
    "laminas/laminas-filter": "^2.5"
  },
  "require-dev": {
    "symfony/var-dumper": "^7.2",
    "phpunit/phpunit": "^11.4",
    "phpspec/prophecy-phpunit": "^2.0"
  }
}
